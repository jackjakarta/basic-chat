{
  "name": "elchat",
  "version": "1.6.10",
  "private": true,
  "scripts": {
    "dev": "ENVIRONMENT=development op run --env-file=\".env.op\" --no-masking -- next dev --hostname 127.0.0.1",
    "dev:turbo": "pnpm dev --turbo",
    "dev:email": "email dev --port 3001",
    "build": "next build",
    "build:dev": "ENVIRONMENT=development op run --env-file=\".env.op\" --no-masking -- next build",
    "start": "next start",
    "start:dev": "ENVIRONMENT=development op run --env-file=\".env.op\" --no-masking -- next start",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "types": "tsc --noEmit",
    "lint": "next lint",
    "test": "vitest --run",
    "checks": "chmod +x $(git rev-parse --show-toplevel)/scripts/checks.sh && $(git rev-parse --show-toplevel)/scripts/checks.sh",
    "pipeline": "chmod +x $(git rev-parse --show-toplevel)/scripts/pipeline.sh && $(git rev-parse --show-toplevel)/scripts/pipeline.sh",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "ENVIRONMENT=development op run --env-file=\".env.op\" -- tsx ./src/db/migrate.ts",
    "db:seed": "ENVIRONMENT=development op run --env-file=\".env.op\" -- tsx ./src/db/seed/index.ts",
    "db:customers": "ENVIRONMENT=development op run --env-file=\".env.op\" -- tsx ./src/db/create-customers.ts",
    "db:studio": "ENVIRONMENT=development op run --env-file=\".env.op\" -- drizzle-kit studio --config=src/db/drizzle.config.ts"
  },
  "dependencies": {
